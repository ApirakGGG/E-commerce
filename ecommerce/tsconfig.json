{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/products/SetColor.tsx", "app/chat/ChatBox.jsx", "app/chat/Chat.jsx", "app/api/chat/route.js", "app/chat/page.js", "app/components/reset/enter-token/page.jsx", "app/components/reset/create-user/route.js", "app/api/edituser/route.ts", "app/api/edituser/[id]/route.ts", "app/category/Category.tsx"],
  "exclude": ["node_modules"]
}
